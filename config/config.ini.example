; THIS MUST BE A PUBLIC IP ADDRESS WITH PROPER PORT FORWARDING
HOST_IP = 213.19.23.123

; TIMEZONE
TIMEZONE = America/New_York

; COMMAND PREFIX
PREFIX = '.'

[DB]
; DATABASE CREDENTIALS
host = 172.16.12.160
username = username
password = password
port = 3306

; DATABASE MAINTENANCE QUERIES
; Remove any pokestops that have been upgraded to gyms daily.
Remove_Upgraded_Pokestops = ENABLED
; Remove any pokestops not seen in 24 hours that may have been removed.
Remove_Unseen_Pokestops = DISABLED

[TOKENS]
